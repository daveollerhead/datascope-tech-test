\n    setPagination(json);\r\n  };\r\n\r\n  useEffect(() => {\r\n    loadGames(pagination.page, pagination.pageSize);\r\n  }, []);\r\n\r\n  const handleDelete = async (game) => {\r\n    const state = games;\r\n    const newGames = games.filter((m) => m.id !== game.id);\r\n    setGames(newGames);\r\n\r\n    try {\r\n      await GamesService.remove(game.id);\r\n    } catch (ex) {\r\n      if (!ex.response || ex.response.status !== 404) {\r\n        toast.error(\"Sorry something has gone wrong\");\r\n        setGames(state);\r\n        return;\r\n      }\r\n\r\n      toast(\"Looks like that game had already been deleted....\");\r\n    }\r\n\r\n    let page = pagination.page;\r\n    if (games.length <= 1 && pagination.page > 1) {\r\n      page--;\r\n    }\r\n\r\n    loadGames(page, pagination.pageSize);\r\n  };\r\n\r\n  const handleEdit = (game) => {\r\n    return history.push(`/game/${game.id}`);\r\n  };\r\n\r\n  const handlePageChange = async (page) => {\r\n    setPagination({ page: page, ...pagination });\r\n    await loadGames(page, pagination.pageSize);\r\n  };\r\n\r\n  const handlePageSizeChange = async ({ currentTarget: input }) => {\r\n    setPagination({ pageSize: input.value, ...pagination });\r\n    await loadGames(pagination.page, input.value);\r\n  };\r\n\r\n  if (!games || games.length < 1) {\r\n    return (\r\n      <Fragment>\r\n        <h2>Games Rating App</h2>\r\n        <p>Currently there are no rated games, why not add one</p>\r\n      </Fragment>\r\n    );\r\n  }\r\n\r\n  return (\r\n    <Fragment>\r\n      <h2>Games Rating App</h2>\r\n      <Table striped bordered hover>\r\n        <thead>\r\n          <tr>\r\n            <th>ID</th>\r\n            <th>Title</th>\r\n            <th>Description</th>\r\n            <th>Released</th>\r\n            <th>Rating</th>\r\n            <th></th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          {games.map((x) => (\r\n            <tr key={x.id}>\r\n              <td>{x.id}</td>\r\n              <td>{x.name}</td>\r\n              <td>{x.description}</td>\r\n              <td>{Moment(x.releasedAt).format(\"DD/MM/yyyy\")}</td>\r\n              <td>{x.rating}</td>\r\n              <td>\r\n                <ButtonGroup>\r\n                  <Button\r\n                    onClick={() => handleEdit(x)}\r\n                    variant=\"outline-primary\"\r\n                    size=\"sm\"\r\n                  >\r\n                    Edit\r\n                  </Button>\r\n                  <Button\r\n                    onClick={() => handleDelete(x)}\r\n                    variant=\"outline-danger\"\r\n                    size=\"sm\"\r\n                  >\r\n                    Delete\r\n                  </Button>\r\n                </ButtonGroup>\r\n              </td>\r\n            </tr>\r\n          ))}\r\n        </tbody>\r\n      </Table>\r\n      <div className=\"row\">\r\n        <div className=\"col-md-3\">\r\n          <PaginationComponent\r\n            pagination={pagination}\r\n            onPageChange={handlePageChange}\r\n          />\r\n        </div>\r\n        <div className=\"offset-md-6\">\r\n          <Form.Group controlId=\"pageSize\">\r\n            <Form.Row>\r\n              <Col>\r\n                <Form.Label>page size</Form.Label>\r\n              </Col>\r\n              <Col>\r\n                <Form.Control as=\"select\" onChange={handlePageSizeChange}>\r\n                  <option>5</option>\r\n                  <option>10</option>\r\n                  <option>25</option>\r\n                </Form.Control>\r\n              </Col>\r\n            </Form.Row>\r\n          </Form.Group>\r\n        </div>\r\n      </div>\r\n    </Fragment>\r\n  );\r\n}\r\n\r\nexport default Games;\r\n","C:\\src\\datascope\\DatascopeTest\\Client\\src\\components\\navBar.jsx",[],"C:\\src\\datascope\\DatascopeTest\\Client\\src\\services\\gamesService.js",[],"C:\\src\\datascope\\DatascopeTest\\Client\\src\\components\\common\\pagination.jsx",[],"C:\\s